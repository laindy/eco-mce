{
  "name": "appname-skeleton",
  "version": "1.0.0",
  "description": "skeleton service",
  "author": "Tiavina rakotobe",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "prisma generate && prisma migrate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "prisma generate && prisma migrate deploy && prisma db seed && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --collectCoverageFrom=!**/dto/* --collectCoverageFrom=!**/enum/* --collectCoverageFrom=!**/database/* --collectCoverageFrom=!**/type/* --collectCoverageFrom=!**/type/**/* --collectCoverageFrom=!**/types/* --collectCoverageFrom=!**/*module.ts --collectCoverageFrom=!**/*main.ts --collectCoverageFrom=!**/*resolver.ts --collectCoverageFrom=!**/config/*",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:seed": "prisma db seed",
    "prisma:docs": "prisma-docs-generator serve"
  },
  "license": "ISC",
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "sharp": "^0.32.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.5",
    "@magidoc/cli": "^4.1.2",
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.8.3",
    "prisma": "^5.15.0",
    "prisma-docs-generator": "^0.8.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/main.ts",
      "!**/mock/**",
      "!**/type/**",
      "!**/types/**",
      "!**/*resolver.ts",
      "!**/*index.ts",
      "!**/*.module.ts",
      "!**/*.controller.ts",
      "**/*.resolver.ts",
      "!**/*.type.ts",
      "!**/*.dto.ts",
      "!**/prisma.service.ts"
    ],
    "coverageDirectory": "../coverage",
    "testResultsProcessor": "jest-sonar-reporter",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "_moduleAliases": {
    "@": "src"
  }
}
